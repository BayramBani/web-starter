{
  "devDependencies": {
    "clean-dir": "^1.0.3",
    "parcel-bundler": "^1.12.5"
  },
  "scripts": {
    "start": "parcel serve --port 8000 -d dev src/index.html",
    "watch": "parcel watch -o bundle src/index.js",
    "build": "parcel build -o bundle src/index.js",
    "prod": "parcel build src/index.html",
    "clean": "yarn run cleandir dist && cleandir dev"
  },
  "dependencies": {
    "bootstrap": "^5.1.1",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  }
}
